{
	"description": "__MSG_extText__",
	"manifest_version": 2,
	"name": "The Stream Detector",
	"default_locale": "en",
	"version": "1.8.0",
	"permissions": [
		"webRequest",
		"*://*/*.m3u8",
		"*://*/*.m3u8?*",
		"*://*/*.mpd",
		"*://*/*.mpd?*",
		"*://*/*.f4m",
		"*://*/*.f4m?*",
		"*://*/*.ism",
		"*://*/*.ism/*",
		"*://*/*.vtt",
		"*://*/*.vtt/*",
		"menus",
		"notifications",
		"activeTab",
		"clipboardWrite",
		"storage"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"icons": {
		"48": "data/icon-dark-48.png",
		"96": "data/icon-dark-96.png"
	},
	"options_ui": {
		"page": "options.html"
	},
	"browser_action": {
		"browser_style": true,
		"theme_icons": [
			{
				"dark": "data/icon-dark-96.png",
				"light": "data/icon-light-96.png",
				"size": 96
			},
			{
				"dark": "data/icon-dark-48.png",
				"light": "data/icon-light-48.png",
				"size": 48
			},
			{
				"dark": "data/icon-dark-16.png",
				"light": "data/icon-light-16.png",
				"size": 16
			}
		]
	}
}